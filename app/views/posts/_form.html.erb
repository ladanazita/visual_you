<%= form_for(@post, html: {multipart: true}) do |f| %>
  <div class='field'>
    <%= f.text_field :title, placeholder: "Create board..." %>
    <span>
      <%= f.file_field :picture%>
      <%= f.hidden_field :user_id, value: current_user.id %>
    </span>
    <%= f.submit "Board" %>
  </div>
<%end%>